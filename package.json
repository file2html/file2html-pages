{
  "name": "file2html-pages",
  "version": "0.1.0",
  "description": "Apple Pages engine for file2html",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib/*.*",
    "lib/**/*.*"
  ],
  "scripts": {
    "test": "./node_modules/.bin/jest --no-cache --config=./node_modules/file2html-dev-tools/jest/config.json",
    "lint": "./node_modules/.bin/tslint \"src/**/*.ts\" \"test/unit/**/*.ts\"",
    "build": "npm run lint && rm -rf lib && ./node_modules/.bin/tsc"
  },
  "dependencies": {
    "file2html": "^0.2.0",
    "file2html-archive-tools": "git+https://github.com/file2html/file2html-archive-tools.git#v1.0.0",
    "file2html-xml-tools": "git+https://github.com/file2html/file2html-xml-tools.git#v1.0.0"
  },
  "devDependencies": {
    "@types/node": "7.0.39",
    "file2html-dev-tools": "git+https://github.com/file2html/file2html-dev-tools.git#v1.0.0",
    "typescript": "2.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/file2html/file2html-pages.git"
  },
  "keywords": [
    "file2html",
    "Apple",
    "Pages",
    ".pages"
  ],
  "author": "webschik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/file2html/file2html-pages/issues"
  },
  "homepage": "https://github.com/file2html/file2html-pages#readme"
}
